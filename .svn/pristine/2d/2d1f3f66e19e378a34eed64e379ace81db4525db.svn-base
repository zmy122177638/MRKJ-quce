*{margin:0;padding:0;}
html,body{height: 100%;}
.answer {
    position: relative;
    min-height: 100%;
    min-height: 100vh;
}
.answer_container{
    min-height:100%;
    min-height:100vh;
    box-sizing: border-box;
}
.answer_test_wrap{
    display: flex;
    min-height:100%;
    flex-direction: column;
}
/* 头部 */
.answer_test_header{
    margin:0.3rem 0.2rem;
    background:url('../images/cstest/bj_dati.png') no-repeat;
    background-size:100% 100%;
    padding:0.3rem;
    box-sizing: border-box;
    flex-shrink: 0;
    font-size:0.32rem;
    color:#333333;
}
.answer_num{font-size:0.24rem;color:#666666;}
/* 进度条 */
.answer_progres{display: flex;align-items: center;padding:0 0.2rem 0.3rem;justify-content: space-between;}
.answer_progres_t{font-size:0.26rem;color:#333333;}
.answer_progres_wrap{
    position: relative;
    height:0.15rem;
    width:5.6rem;
    background-color:#E0E0E0;
    border-radius: 0.12rem;
    -webkit-border-radius: 0.12rem;
    -moz-border-radius: 0.12rem;
    -ms-border-radius: 0.12rem;
    -o-border-radius: 0.12rem;
}
.answer_progres_active{
    position: absolute;
    background-color:#ff5704;
    width:1rem;
    height:0.15rem;
    border-radius: 0.12rem;
    -webkit-border-radius: 0.12rem;
    -moz-border-radius: 0.12rem;
    -ms-border-radius: 0.12rem;
    -o-border-radius: 0.12rem;
    transition: all .3s;
    -webkit-transition: all .3s;
    -moz-transition: all .3s;
    -ms-transition: all .3s;
    -o-transition: all .3s;
}
.answer_progres_active::after{
    content: '';
    width:0.24rem;
    height:0.24rem;
    display: inline-block;
    border-radius: 50%;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%;
    position: absolute;
    right:0;top:50%;
    margin-top:-0.12rem;
    background-color:#ff5704;
}
.answer_progres_active_promt{
    position: absolute;
    bottom:-0.45rem;
    right:-0.3rem;
    font-size:0.26rem;
    color:#ff5704;
}
.answer_c_time{font-size:0.24rem;color:#666666;}
.clock_icon{width:0.32rem;height:0.32rem;vertical-align: middle;}
.answer_test_promt{font-size:0;padding:0.1rem;margin-left:0.3rem;}
.why_icon{width:0.32rem;height:0.32rem;vertical-align: middle;}
/* 身体 */
.answer_body{height:100%;position: relative;}
.answer_progres_container{

}
.answer_progres_timu{
    padding:0.05rem 0.4rem;
    font-size:0.36rem;
    color:#ffffff;
    background-color:#ff5704;
    display: inline-block;
    border-radius: 0 0.7rem 0.7rem 0;
    -webkit-border-radius: 0 0.7rem 0.7rem 0;
    -moz-border-radius: 0 0.7rem 0.7rem 0;
    -ms-border-radius: 0 0.7rem 0.7rem 0;
    -o-border-radius: 0 0.7rem 0.7rem 0;
    margin:0.3rem 0 0.6rem;
}
.answer_animation_right_in{animation:moveRightin .2s ease forwards;}
.answer_animation_right_out{animation:moveRightout .2s ease forwards;}
.answer_animation_left_in{animation:moveLeftin .2s ease forwards;}
.answer_animation_left_out{animation:moveLeftout .2s ease forwards;}
@keyframes moveLeftout{
    0%{left:0;top:0;}
    100%{left:-100%}
}
@keyframes moveLeftin{
    0%{left:100%;top:0;}
    100%{left:0;top:0;}
}
@keyframes moveRightout{
    0%{right:0;top:0;}
    100%{right:-100%;top:0;}
}
@keyframes moveRightin{
    0%{right:100%;top:0;}
    100%{right:0;top:0;}
}
.answer_timu_item{
    width: 100%;
    box-sizing: border-box;
    background-color:#ffffff;
    transform: translate3d(0,0,0);
    margin-top:0.4rem;
}
.answer_timu_title{padding:0.2rem;font-size:0.36rem;color:#333333;line-height: 1.4;font-weight: 700;}
.answer_timu_list{list-style: none;}
.answer_timu_list li{
    padding:0.3rem 0.2rem;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size:0.3rem;
    color:#333333;
    box-sizing: border-box;
    border:2px solid transparent;
}
.answer_timu_list li.on{
    border-color:#02A1FD;
    position: relative;
}
.answer_timu_list li.on::after{
    content: '';
    width:0.44rem;
    height:0.44rem;
    background: url('../images/cstest/gou.png') no-repeat;
    background-size:100% 100%;
    position: absolute;
    right:0;top:0;
}
.answer_timu_list li.on .answer_timu_l{
    background-color:#03A0FC;
    color:#ffffff;
}
.answer_timu_c{flex:1;}
.answer_timu_check{
    width:0.32rem;
    height:0.32rem;
    border:0.04rem solid #ffdd2b;
    border-radius: 50%;
    position: relative;
    flex-shrink: 0;
}
.answer_timu_checked::after{
    content:'';
    width:0.28rem;
    height:0.28rem;
    position: absolute;
    border-radius: 50%;
    background-color:#d3940d;
    left:50%;
    top:50%;
    margin-left:-0.14rem;
    margin-top:-0.14rem;
}
.answer_timu_l{
    width:0.68rem;
    height:0.68rem;
    line-height: 0.68rem;
    text-align: center;
    border-radius: 50%;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%;
    background-color:#E1E1E1;
    font-size:0.36rem;
    color:#333333;
    margin-right:0.24rem;
}
/* 脚 */
.answer_footer{
    flex-shrink: 0;
    margin:0.6rem 0;
    display: flex;
    align-items: center;
    justify-content: center;
    padding:0 0.26rem;
    box-sizing: border-box;
}
.answer_footer div:nth-child(2){margin-left:0.4rem;}
.answer_footer_prevBtn{
    height:0.8rem;
    line-height: 0.8rem;
    font-size:0.32rem;
    text-align: center;
    color:#ff5704;
    flex: 1;
    border: 2px solid #ff5704;
    border-radius: 0.12rem;
    -webkit-border-radius: 0.12rem;
    -moz-border-radius: 0.12rem;
    -ms-border-radius: 0.12rem;
    -o-border-radius: 0.12rem;
}
/* 测试提示 */
.test_promt_container{
    width:100%;
    height:100%;
    background-color:rgba(0,0,0,0.7);
    position: fixed;
    z-index: 99;
    left:0;
    top:0;
}
.test_promt_wrap{
    width:90%;
    background-color:#ffffff;
    position: absolute;
    left:50%;
    top:10%;
    transform:translateX(-50%);
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    border-radius: 0.1rem;
}
.test_promt_title{ 
    padding:0.5rem 0 0.4rem;
    font-size:0.36rem;
    color:#333333;
    font-weight: 700;
    text-align: center;
}
.a_color{background-color:#ffdd2b;color:#000000;font-size: 0.32rem;}
.test_promt_content{padding:0 0.5rem;}
.test_promt_content p{margin-bottom: 0.5rem;font-size:0.28rem;color:#666666;line-height: 1.56;}
.close_test_promt{
    width:0.48rem;
    height:0.48rem;
    background:url('../images/close_promt.png') no-repeat;
    background-size:100% 100%;
    position: absolute;right:0.2rem;top:0.2rem;
}
.test_promt_confirm{
    width:70%;
    height:0.8rem;
    line-height: 0.8rem;
    text-align: center;
    background-color: #ffdd2b;
    color:#ffffff;
    font-size:0.32rem;
    margin:0 auto 0.4rem;
    -webkit-tap-highlight-color: transparent;
}